let allowed atoms be h, o, N.
let additional species be N2.
let temperature exponent be n_k.
let order of reaction be n.
let units for A be [ cm^(3(n-1)) / ( ms * mole^(n-1) * K^n_k ) ] . 	/* units modified from second to millisecond */
let units for E be [ J / mole ].	/* units modified from KJ to J */



1f: O2 + H            -> OH + O                { a = 2.000E+11	n = 0	E = 70300	}
