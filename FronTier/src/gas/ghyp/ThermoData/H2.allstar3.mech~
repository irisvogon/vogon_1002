6f: O + O + M'        -> O2 + M'               { a = 2.900E+14	n = -1	E = 0	}
7f: H + OH + M'       -> H2O + M'              { a = 2.200E+19	n = -2	E = 0	}
8f: H + O2 + M'       -> HO2 + M'              { a = 2.300E+15	n = -0.8	E = 0	}
9f: HO2 + H           -> OH + OH               { a = 1.500E+11	n = 0	E = 4200	}
10f: HO2 + H          -> H2 + O2               { a = 2.500E+10	n = 0	E = 2900	}
11f: HO2 + H          -> H2O + O               { a = 3.000E+10	n = 0	E = 7200	}
12f: HO2 + O          -> OH + O2               { a = 1.800E+10	n = 0	E = -1700	}
